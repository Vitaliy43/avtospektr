html { height: 100%;  font: .75em/1.5 Verdana,Arial,Helvetica, sans-serif; }
body { height: auto !important; height: 100%; min-height: 100%; margin: 0; padding: 0;  } /* body height 100% */
a {color: #000; text-decoration: none;}
h1,h2 {padding-left: 5px;}
.breadcrumbs {padding-left: 5px;}
#wrapper {
	background:#0a0c0b;
	height:100%;
	width:100%;
	background-repeat: repeat-y;
	min-height:100%;
    min-width: 1280px;



}

.left_green {

	overflow:hidden;

	
}

.right_green {
	
	overflow:hidden;
	 
}


#container {
	
	background:white;

	width:1000px;
	
	
}

#header {

	background:url(../../images/avtospektr/header.jpg);
	height:212px;
	margin:0px;
    cursor: pointer;
    background-repeat: no-repeat;

	
}

#firms {
	
	background:url(../../images/avtospektr/list_firms.jpg);
	width:100%;
	margin:0px;
	padding:0px;
	height:52px;
    background-repeat: no-repeat;
	
}


#nav_bar {
margin-top:1px;	
	
}

.label {

	background:url(../../images/avtospektr/label_background.png);
	margin:0px;
	padding:0px;
	height:41px;
	border-right:1px solid #617156;
	text-align:left;

}

.label font {
	padding-left:30px;
	font-size:17px;
	font-family:Arial;
	font-weight:bold;
	color:#000;
	text-shadow: 1px 1px 2px white;
}

#sidebar {
	
	float:left;
	width:301px;
}

#menu_catalog {border: 1px solid #bde0a3;}



.menu_option span {

    padding-left: 30px;
    padding-bottom: 10px;
    padding-top: 10px;
    color: #000;
    font-size: 13px;

}

.fio span {

    padding-left: 30px;
    padding-bottom: 10px;
    padding-top: 10px;
    color: #000;
    font-size: 13px;

}

.profile_option div {

    margin-left: 10px;
    padding-bottom: 0px;
    padding-top: 0px;
    color: #000;
    font-size: 13px;

}

.profile {padding-bottom: 0px;}

.profile_option img{


    padding-right: 10px;


}

.menu_option:hover {cursor: pointer;}

.fio:hover {cursor: default;}


.submenu1_option {background: #def1d9;}
.submenu2_option {background: #caf1c0;}

.submenu1_option span {

    padding-left: 50px;
    padding-bottom: 0px;
    padding-top: 0px;
    color: #000;
    font-size: 12px;


}

.submenu2_option span {

    padding-left: 70px;
    padding-bottom: 0px;
    padding-top: 0px;
    color: #000;
    font-size: 11px;


}

.submenu1_option .menu_arrow img {padding-top: 0px; padding-bottom: 0px; padding-right: 0.7em;}
.submenu2_option .menu_arrow img {padding-top: 0px; padding-bottom: 0px; padding-right: 0.7em;}

.menu_arrow img {padding-top: 10px; padding-bottom: 10px; padding-right: 8px;}
.menu_row {border-bottom: 1px solid #7f7f7f;border-top: 1px solid #bde0a3;}

.auth {vertical-align: middle;}
#authorization font {vertical-align: middle;}
.auth_input {width: 230px;}
.authorization div {width: 100%;padding-left: 30px;padding-top: 4px;color: #000;
    font-size: 13px;}
.authorization input {border-right: 1px solid #bde0a3;border-bottom: 1px solid #bde0a3;border-left: 1px solid #7f7f7f; border-top: 1px solid #7f7f7f;}

.reg_forget {text-align: left;}
.reg_forget div {margin-top: -10px;}
.reg_forget div a {margin-left: -60px;}

.auth_submit {background: #bde0a3; font-size: 14px;width: 100px; height: 25px;}
#menu_authorization {margin-bottom: 10px;border-right: 1px solid #bde0a3;}

#container_content {padding: 10px; margin-left: 300px; }
