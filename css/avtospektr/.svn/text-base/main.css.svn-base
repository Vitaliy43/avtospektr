#our_partners {margin-top: 70px;padding-left: 25px;}
#list_partners {margin-top: 25px;}
#partners {margin-bottom: 20px;max-height: 300px;}
#footer {color: #446027;font-size: 13px;font-weight: bold; margin-top: 15px;margin-bottom: 15px;}

.hidden_menu {display: none;position: absolute;z-index: 2;left:300px;width: 250px;background: white;border: 0.1em solid #bde0a3}
